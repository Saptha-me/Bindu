{
    "author": "raahul@saptha.me",
    "name": "first Agent",
    "description": "A basic AI agent for simple question answering",
    "version": "1.0.0",
    "recreate_keys": true,
    "skills": [
      {
        "name": "question-answering",
        "description": "Ability to answer basic questions",
        "version": "1.0.0"
      },
      {
        "name": "question-answering2",
        "description": "Ability to answer basic questions",
        "version": "1.0.0"
      }
    ],
    "capabilities": {
      "reasoning": true,
      "code_execution": false,
      "web_search": false,
      "file_operations": false,
      "multimodal": false
    },
    "agent_trust": "low",
    "kind": "agent",
    "debug_mode": true,
    "debug_level": 2,
    "monitoring": false,
    "telemetry": false,
    "num_history_sessions": 5,
    "documentation_url": "https://docs.saptha.me/agents/simple",
    "extra_metadata": {
      "domain": "general",
      "specialization": "basic_qa"
    },
    "auth": {
      "enabled": false,
      "domain": "your-tenant.auth0.com",
      "audience": "https://api.bindu.ai",
      "algorithms": ["RS256"],
      "require_permissions": false,
      "permissions": {
        "message/send": ["agent:write"],
        "tasks/get": ["agent:read"],
        "tasks/cancel": ["agent:write"],
        "tasks/list": ["agent:read"],
        "contexts/list": ["agent:read"],
        "tasks/feedback": ["agent:write"]
      }
    },
    "deployment": {
      "url": "http://localhost:8030",
      "expose": true,
      "protocol_version": "1.0.0",
      "proxy_urls": ["https://simple.saptha.me"],
      "cors_origins": ["*"]
    },
    "storage": {
      "type": "memory"
    },
    "scheduler": {
      "type": "memory"
    }
  }